{
  "name": "create-gameplan",
  "version": "0.0.8",
  "description": "A CLI to initialize projects from a git repo template",
  "keywords": [
    "template",
    "git",
    "boilerplate",
    "gameplan"
  ],
  "license": "MIT",
  "author": "Keith Smith <npm@ksmithut.com>",
  "files": [
    "LICENSE",
    "README.md",
    "src/index.js",
    "src/bin/*",
    "src/lib/*"
  ],
  "bin": "src/bin/cli.js",
  "scripts": {
    "format": "prettier-eslint 'src/**/*.js' --write",
    "lint": "eslint 'src/**/*.js' && prettier-eslint 'src/**/*.js' --list-different",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "inquirer": "^7.0.0",
    "rimraf": "^3.0.0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "assert-dir-equal": "^1.1.0",
    "eslint": "^6.2.2",
    "eslint-config-standard": "^14.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "prettier-eslint-cli": "^5.0.0"
  }
}
