{
  "name": "create-gameplan",
  "version": "0.0.6",
  "description": "A CLI to initialize projects from a git repo template",
  "keywords": [
    "template",
    "git",
    "boilerplate",
    "gameplan"
  ],
  "license": "MIT",
  "author": "Keith Smith <npm@ksmithut.com>",
  "files": [
    "LICENSE",
    "README.md",
    "src/index.js",
    "src/bin/*",
    "src/lib/*"
  ],
  "bin": "src/bin/cli.js",
  "scripts": {
    "format": "prettier-eslint 'src/**/*.js' --write",
    "lint": "eslint 'src/**/*.js' && prettier-eslint 'src/**/*.js' --list-different",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "inquirer": "^6.3.1",
    "rimraf": "^2.6.3",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "assert-dir-equal": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "prettier-eslint-cli": "^4.7.1"
  }
}
